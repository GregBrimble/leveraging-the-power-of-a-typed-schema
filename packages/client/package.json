{
  "name": "client",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@tailwindcss/custom-forms": "0.2.1",
    "apollo-boost": "0.4.7",
    "autoprefixer": "9.7.3",
    "cssnano": "4.1.10",
    "graphql": "14.5.8",
    "lodash": "^4.17.15",
    "moment": "2.24.0",
    "postcss-cli": "6.1.3",
    "postcss-import": "12.0.1",
    "postcss-nesting": "7.0.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.3.0",
    "tailwindcss": "1.1.4",
    "typescript": "3.7.3"
  },
  "scripts": {
    "prebuild": "NODE_ENV=production npm run build:styles",
    "build": "react-scripts build",
    "build:styles": "postcss src/index.css -o src/styles.css",
    "clean": "rimraf build; npm run clean:styles",
    "clean:node": "rimraf node_modules package-lock.json",
    "clean:styles": "rimraf src/styles.css",
    "start": "concurrently \"npm run build:styles -- --watch\" \"react-scripts start\"",
    "storybook": "concurrently \"npm run build:styles -- --watch\" \"start-storybook -p 3001\"",
    "lint": "eslint --ext js,ts,tsx src",
    "format": "npm run lint -- --fix",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-info": "5.2.8",
    "@storybook/addon-knobs": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/preset-typescript": "1.2.0",
    "@storybook/react": "5.2.8",
    "@types/jest": "24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/node": "12.12.17",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "babel-loader": "8.0.6",
    "concurrently": "5.0.1",
    "react-docgen-typescript-loader": "3.6.0",
    "ts-loader": "6.2.1"
  }
}
